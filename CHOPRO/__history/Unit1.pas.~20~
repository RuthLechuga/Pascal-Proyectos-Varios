{$R *.dfm}

procedure TForm1.Acercade1Click(Sender: TObject);
begin
Form5.Show;
Form1.Hide;
end;

procedure TForm1.Button1Click(Sender: TObject);
begin
Form2.Show;
Form1.hide;
end;

procedure TForm1.Button2Click(Sender: TObject);
begin
Form3.show;
Form1.hide;
end;


procedure TForm1.Button3Click(Sender: TObject);
begin
application.terminate;
end;

procedure TForm1.CalculadoraClick(Sender: TObject);
begin
Form7.Show;
Form1.Hide;
end;

procedure TForm1.Conversiones1Click(Sender: TObject);
begin
Form1.Hide;
Form8.show;
end;

procedure TForm1.Ecuaciones1Click(Sender: TObject);
begin
Form2.pagecontrol1.tabindex:= 0;
form1.Hide;
Form2.show;
end;

procedure TForm1.Gauss1Click(Sender: TObject);
begin
Form2.pagecontrol1.tabindex:= 2;
form1.Hide;
Form2.show;
end;

