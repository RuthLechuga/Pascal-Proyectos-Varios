  Dialogs, Menus, XPMan, StdCtrls, jpeg, ExtCtrls;

type
  TForm1 = class(TForm)
    Button1: TButton;
    Button2: TButton;
    XPManifest1: TXPManifest;
    MainMenu1: TMainMenu;
    Fsica1: TMenuItem;
    Button3: TButton;
    Mru1: TMenuItem;
    MRUV1: TMenuItem;
    Acercade1: TMenuItem;
    mATE: TMenuItem;
    M1: TMenuItem;
    MCUV1: TMenuItem;
    iroParablico1: TMenuItem;
    N1: TMenuItem;
    Calculadora: TMenuItem;
